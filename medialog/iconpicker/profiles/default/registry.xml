<registry>
<records interface="medialog.iconpicker.interfaces.IIconPickerSettings">
<value key="iconset">fontawesome</value>
<value key="loadbootstrap">True</value>
<value key="rows">5</value>
<value key="cols">5</value>
<value key="placement">right</value>
</records>




<record name="plone.app.tiles">
 <field type="plone.registry.field.List">
 <title>Tiles</title>
 <value_type type="plone.registry.field.TextLine" />
 </field>
 <value purge="false">
   <element>medialog.iconpicker.multitile</element>
 </value>
 <value purge="false">
   <element>medialog.iconpicker.tile"</element>
 </value>
 </record>


<records prefix="plone.app.mosaic.app_tiles.icontile"
 interface="plone.app.mosaic.interfaces.ITile">
 <value key="name">medialog.iconpicker.tile</value>
